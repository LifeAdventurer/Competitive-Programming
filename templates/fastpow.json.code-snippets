    "prefix": "fastpow template",
    "body": [
      "int fastpow(int x, int y, int mod){",
      "	int res = 1;",
      "	while(y){",
      "		if(y & 1) res = (1LL * res * x) % mod;",
      "		y >>= 1;",
      "		x = (1LL * x * x) % mod;",
      "	}",
      "	return res;",
      "}",
